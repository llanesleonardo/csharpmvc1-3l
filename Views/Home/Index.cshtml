@model List<Tresele.crmbe.Models.WeatherForecast>
@{
    Layout = "_Layout";
}

<h2>Weather Forecast</h2>

<table class="table">
    <thead>
        <tr>
            <th>Date</th>
            <th>Temp. C</th>
            <th>Temp. F</th>
            <th>Summary</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var forecast in Model)
        {
            <tr>
                <td>@forecast.Date</td>
                <td>@forecast.TemperatureC</td>
                <td>@forecast.TemperatureF</td>
                <td>@forecast.Summary</td>
            </tr>
        }
    </tbody>
</table>
